<%= form_with(model: @feature) do |form| %>
<div class="field mb-3">
    <%= form.label :name %>
    <%= form.text_field :name, class:"form-control"%>
  </div>

  <div class="field mb-3">
    <%= form.label :code %>
    <%= form.text_field :code, class:"form-control" %>
